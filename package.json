{
	"name": "parrot",
	"displayName": "DiscordParrot",
	"description": "Parrots discord messages between vscode and your discord server",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:parrot.start"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "parrot.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "parrot.start",
				"title": "Parrot Start"
			}
		]
	},
	"scripts": {
		"build": "webpack --mode development",
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"start": "npm run build & webpack-dev-server --open --mode development && node bot.js"
	},
	"dependencies": {
    "node-fetch": "^2.6.1",
		"bootstrap": "^5.1.3",
		"discord.io": "https://github.com/woor/discord.io/tarball/gateway_v6",
		"dotenv": "^16.0.0",
		"jquery": "^3.6.0",
		"popper.js": "^1.16.1",
		"winston": "^3.7.2",
		"xhr2": "^0.2.1"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.66.0",
		"@vscode/test-electron": "^2.1.3",
		"dotenv-webpack": "^2.0.0",
		"eslint": "^8.11.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^3.2.0",
		"glob": "^7.2.0",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.9.0",
		"mocha": "^9.2.2",
		"style-loader": "^1.0.0",
		"typescript": "^4.5.5",
		"webpack": "4.39.3",
		"webpack-cli": "^3.3.8",
		"webpack-dev-server": "3.8.0"
	}
}
